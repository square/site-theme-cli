{
  "name": "square-online-cli",
  "version": "0.0.0",
  "description": "Square Online CLI",
  "author": "Rajiv Puri",
  "bin": {
    "square-online-cli": "./bin/run.js"
  },
  "type": "module",
  "homepage": "https://github.com/squareup/ecom-square-online-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/squareup/ecom-square-online-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/prompts": "^1.2.3",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@vitest/coverage-v8": "^0.34.6",
    "axios": "^1.6.8",
    "chokidar": "^3.5.3",
    "figlet": "^1.6.0",
    "form-data": "^4.0.0",
    "ink": "^4.2.0",
    "ink-confirm-input": "^2.0.0",
    "ink-link": "^3.0.0",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "istanbul-badges-readme": "^1.8.5",
    "js-yaml": "^4.1.0",
    "lorem-ipsum": "^2.0.8",
    "mime-types": "^2.1.35",
    "parse-gitignore": "^2.0.0",
    "react": "^18.2.0",
    "react-ink": "^6.5.4",
    "square": "^28.0.1",
    "url-join": "^5.0.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/figlet": "^1.5.6",
    "@types/ink": "^2.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.18.34",
    "@types/parse-gitignore": "^1.0.2",
    "@types/react": "^18.2.12",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/typescript-estree": "^5.59.11",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-spellcheck": "^0.0.20",
    "husky": "^8.0.3",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5",
    "vitest": "^0.34.6"
  },
  "oclif": {
    "bin": "square-online-cli",
    "dirname": "square-online-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "theme": {
        "description": "Sub commands for theme development"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b --force",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "husky install && npm run build",
    "test": "vitest --silent run",
    "coverage": "vitest run --coverage.enabled",
    "coverage:ci": "vitest run --coverage.enabled --coverage.reporter='json-summary'",
    "make-coverage-badges": "istanbul-badges-readme",
    "version": "oclif readme && git add README.md",
    "generate-readme": "yarn build && oclif readme"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ecom-square-online-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
